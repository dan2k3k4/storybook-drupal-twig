    "webpack-dev": "drush webpack:serve --docker --dev-server-host=cli",
    "lint:css": "stylelint --ignore-path .eslintignore \"**/*.css\"",
    "lint:css:fix": "yarn run lint:css --fix",
    "lint:js": "eslint --ignore-path .eslintignore . --ext ts --ext js",
    "lint:js:fix": "yarn run lint:js --fix",
    "lint:fix": "yarn lint:css:fix && yarn lint:js:fix",
    "theme-watch": "webpack -p --config webpack.config.js --mode development --watch --progress"
  },
